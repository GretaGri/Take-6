<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.take6">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".FirstPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:parentActivityName=".FirstPageActivity" />

        <activity
            android:name=".MainActivityTen"
            android:parentActivityName=".FirstPageActivity"
            android:windowSoftInputMode="stateHidden|adjustPan"/>

        <activity
            android:name="AboutApplication"
            android:label="About application"
            android:parentActivityName=".FirstPageActivity" />

        <activity
            android:name=".HowToPlay"
            android:label="How to play"
            android:parentActivityName=".FirstPageActivity" />

    </application>

</manifest>